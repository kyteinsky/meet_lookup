{
  "name": "GMeet Lookup",
  "description": "Extension to alert you when some important work is said or typed in the Google Meet.",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {},
  "background": {
    "scripts": ["./js/background.js"],
    "persistent": true
  },
  "options_page": "./html/options.html",
  "browser_action": {
    "default_popup": "./html/popup.html"
  },
  "permissions": [
    "tabs",
    "https://meet.google.com/*",
    "storage"
  ]
}
