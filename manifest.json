{
  "name": "GMeet Lookup",
  "description": "Extension to alert you when any important keyword is said or typed in the Google Meet.",
  "version": "1.1",
  "manifest_version": 2,
  "icons": {},
  "background": {
    "scripts": ["./background/background.js"],
    "persistent": true
  },
  "options_page": "./options/options.html",
  "browser_action": {
    "default_popup": "./popup/popup.html"
  },
  "permissions": [
    "tabs",
    "https://meet.google.com/*",
    "storage"
  ]
}
